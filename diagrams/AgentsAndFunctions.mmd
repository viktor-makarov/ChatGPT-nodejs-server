    classDef mainAgentStyle fill:#add8e6,stroke:#0000ff,stroke-width:2px,font-family:Arial;
    classDef launchFunctionStyle fill:#e6e6fa,stroke:#800080,stroke-width:2px,font-family:Arial;
    classDef specializedAgentStyle fill:#90ee90,stroke:#008000,stroke-width:2px,font-family:Arial;
    classDef functionsStyle fill:#ffe4b5,stroke:#ffa500,stroke-width:2px,font-family:Arial;
    class mainAgent mainAgentStyle;
    class launchFunction functionsStyle;
    class specializedAgents specializedAgentStyle;
    class functions functionsStyle;


